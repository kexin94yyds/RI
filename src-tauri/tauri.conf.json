{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "单词记录器",
  "version": "2.1.0",
  "identifier": "com.wordrecorder.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "单词记录器",
        "width": 394,
        "height": 646,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "alwaysOnTop": true,
        "skipTaskbar": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false
  },
  "plugins": {
    "store": {},
    "clipboard-manager": {},
    "notification": {},
    "dialog": {},
    "fs": {}
  }
}

